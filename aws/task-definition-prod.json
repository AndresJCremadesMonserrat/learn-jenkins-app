{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "UdemyLearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "nginx:1.27-alpine",
            "portMappings": [{
                "name" : "ngnix-80-tip",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::904233100756:role/ecsTaskExecutionRole"
}